{
    "title": "DWIN",
    "author": "dwin",
    "description": "迪文科技帮助文档",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "DWIN官网": "http://www.dwin.com.cn"
        }
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "sharing-plus",
        "anchor-navigation-ex",
	"favicon"
    ],
    "pluginsConfig": {
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": false,
            "whatsapp": false,
            "all": [
                "google", "facebook", "weibo", "twitter",
                "qq", "qzone", "linkedin", "pocket"
            ]
        },
	"anchor-navigation-ex": {
            "associatedWithSummary":false,
            "showLevel":true,
            "multipleH1": true,
            "mode": "float",
             "pageTop": {
                  "showLevelIcon": false,
                  "level1Icon": "fa fa-hand-o-right",
                  "level2Icon": "fa fa-hand-o-right",
                  "level3Icon": "fa fa-hand-o-right"
    		}
        },
        "favicon":{
            "shortcut": "./source/images/favicon.jpg",
            "bookmark": "./source/images/favicon.jpg",
            "appleTouch": "./source/images/apple-touch-icon.jpg",
            "appleTouchMore": {
                "120x120": "./source/images/apple-touch-icon.jpg",
                "180x180": "./source/images/apple-touch-icon.jpg"
            }
        }
    }
}

